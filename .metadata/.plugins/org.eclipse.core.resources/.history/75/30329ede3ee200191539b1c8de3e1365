
public class Array<T> extends ManagementPeople<T> {

	@Override
	public void Mostrar() {
		if(this.head == null) {
			System.out.println("Lista Vacia...");
		}
		
		Data<T> p = this.head;
		while (p!=null) {
			getArray((int[])p.value);
			p = p.next;
		}
	}
	
	public void getArray(int[] array) {
		for(int i = 0; i<array.length; i++) {
			System.out.print(" - " + array[i]);
		}
		System.out.println();		
	}
}
